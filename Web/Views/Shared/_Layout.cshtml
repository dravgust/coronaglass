<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    @await RenderSectionAsync("Title", true)

    <environment include="Development">
        <link rel="stylesheet" href="https://code.jquery.com/ui/jquery-ui-git.css" />
        <link rel="stylesheet" href="~/css/metronic.style.css" />
        <link rel="stylesheet" href="~/css/metronic.aside.dark.css" />
        <link rel="stylesheet" href="~/fonts/metronic.fonts.min.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/overrides/aside.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/subheader.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/bootbox.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/style.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    </environment>

    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/metronic.style.css" />
        <link rel="stylesheet" href="~/css/metronic.aside.dark.css" />
        <link rel="stylesheet" href="~/fonts/metronic.fonts.min.css" />
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/overrides/aside.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/subheader.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/bootbox.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/overrides/style.overrides.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    </environment>

    @await RenderSectionAsync("Head", false)

    <script>
        window._root = '@Url.Content("~/")';
    </script>

</head>
<body>
<div class="kt-grid kt-grid--hor kt-grid--root">
    <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--ver kt-page">
        @await RenderSectionAsync("sidebar", false)
        <div class="kt-content kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor" id="kt_content">
            @RenderBody()
        </div>
        <!-- begin:: Footer --><!-- end:: Footer -->
    </div>
</div>

<environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
        <script src="~/metronic/v5/plugins/bootbox/bootbox.min.js"></script>
        <script src="~/metronic/v5/plugins/moment/moment.min.js"></script>
        <script src="~/metronic/v5/plugins/sticky/sticky.min.js"></script>
        <script src="~/metronic/v5/plugins/raphael/raphael.min.js"></script>
        <script src="~/metronic/v5/plugins/js-cookie/js-cookie.js"></script>
        <script src="~/metronic/v5/plugins/popper/popper.js"></script>
        <script src="~/metronic/v5/plugins/tooltip/tooltip.js"></script>
        <script src="~/metronic/v5/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
        <script src="~/metronic/v5/plugins/owl.carousel/owl.carousel.min.js"></script>
        <script src="~/metronic/v5/plugins/bootstrap-daterangepicker/bootstrap-daterangepicker.min.js"></script>
        <script src="~/metronic/v5/plugins/json2/json2.min.js"></script>
        <script src="~/metronic/v5/plugins/event-emitter/event_emitter.js"></script>
        <script src="~/metronic/v5/plugins/event-emitter/event_emitter.wrapped.js"></script>

        <script src="~/metronic/v5/js/init.options.js"></script>
        <script src="~/metronic/v5/js/global/components/base/util.js"></script>
        <script src="~/metronic/v5/js/global/components/base/app.js"></script>
        <script src="~/metronic/v5/js/global/components/base/header.js"></script>
        <script src="~/metronic/v5/js/global/components/base/menu.js"></script>
        <script src="~/metronic/v5/js/global/components/base/offcanvas.js"></script>
        <script src="~/metronic/v5/js/global/components/base/scrolltop.js"></script>
        <script src="~/metronic/v5/js/global/components/base/toggle.js"></script>
        <script src="~/metronic/v5/js/global/layout/layout.js"></script>
    </environment>

    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
        <script src="~/metronic/v5/plugins/bootbox/bootbox.min.js"></script>
        <script src="~/metronic/v5/plugins/moment/moment.min.js"></script>
        <script src="~/metronic/v5/plugins/sticky/sticky.min.js"></script>
        <script src="~/metronic/v5/plugins/raphael/raphael.min.js"></script>
        <script src="~/metronic/v5/plugins/js-cookie/js-cookie.js"></script>
        <script src="~/metronic/v5/plugins/popper/popper.js"></script>
        <script src="~/metronic/v5/plugins/tooltip/tooltip.js"></script>
        <script src="~/metronic/v5/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
        <script src="~/metronic/v5/plugins/owl.carousel/owl.carousel.min.js"></script>
        <script src="~/metronic/v5/plugins/bootstrap-daterangepicker/bootstrap-daterangepicker.min.js"></script>
        <script src="~/metronic/v5/plugins/json2/json2.min.js"></script>
        <script src="~/metronic/v5/plugins/event-emitter/event_emitter.js"></script>
        <script src="~/metronic/v5/plugins/event-emitter/event_emitter.wrapped.js"></script>

        <script src="~/metronic/v5/js/init.options.js"></script>
        <script src="~/metronic/v5/js/global/components/base/util.js"></script>
        <script src="~/metronic/v5/js/global/components/base/app.js"></script>
        <script src="~/metronic/v5/js/global/components/base/header.js"></script>
        <script src="~/metronic/v5/js/global/components/base/menu.js"></script>
        <script src="~/metronic/v5/js/global/components/base/offcanvas.js"></script>
        <script src="~/metronic/v5/js/global/components/base/scrolltop.js"></script>
        <script src="~/metronic/v5/js/global/components/base/toggle.js"></script>
        <script src="~/metronic/v5/js/global/layout/layout.js"></script>
    </environment>

@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
